<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.graffitab.server.persistence.model.Asset" table="asset">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="guid" column="guid" not-null="true"/>

		<many-to-one class="com.graffitab.server.persistence.model.User"
		 name="user"
		 column="user_id"
		 update="false"
		 insert="false"/>

		<property name="assetType" column="asset_type">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.graffitab.server.persistence.model.Asset$AssetType</param>
                <param name="type">12</param>
            </type>
        </property>
     </class>
</hibernate-mapping>
